{
    "name": "revolution/laravel-notification-chatwork",
    "description": "Chatwork notifications channel for Laravel. (v2)",
    "keywords": [
        "chatwork",
        "notification",
        "laravel"
    ],
    "homepage": "https://github.com/kawax/laravel-notification-chatwork/",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomohiro Kaneko",
            "email": "kaneko@e2info.com",
            "homepage": "https://www.e2info.co.jp/",
            "role": "Developer"
        },
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "^6.0",
        "illuminate/notifications": "^5.5",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.5",
        "orchestra/database": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\NotificationChannels\\Chatwork\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Revolution\\NotificationChannels\\Chatwork\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\NotificationChannels\\Chatwork\\ChatworkServiceProvider"
            ]
        }
    }
}
